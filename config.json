{
    "log": {
        "level": "info"
    },
    "dns": {
        "servers": [
            {
                "address": "tls://8.8.8.8"
            }
        ]
    },
    "inbounds": [
        {
            "type": "vless",
            "sniff": true,
            "sniff_override_destination": true,
            "listen": "::",
            "listen_port": 2052,
            "users": [
                {
                    "uuid": "[INPUT_UUID]",
                    "flow": "xtls-rprx-vision"
                }
            ],
            "tls": {
                "enabled": true,
                "server_name": "www.yahoo.com",
                "reality": {
                    "enabled": true,
                    "handshake": {
                        "server": "www.yahoo.com",
                        "server_port": 443
                    },
                    "private_key": "[INPUT_PRIVATE_KEY]",
                    "short_id": [
                        "[INPUT_SHORT_ID]"
                    ]
                }
            }
        },
        {
            "type": "hysteria2",
            "listen": "::",
            "listen_port": 8443,
            "up_mbps": 500,
            "down_mbps": 500,
            "users": [
                {
                    "name": "crooi",
                    "password": "[INPUT_PASSWORD]"
                }
            ],
            "tls": {
                "enabled": true,
                "alpn": [
                    "h3"
                ],
                "min_version":"1.2",
                "max_version":"1.3",
                "certificate_path": "/etc/sing-box/cert.crt",
                "key_path": "/etc/sing-box/private.key"
            }
        },
        {
            "type": "shadowsocks",
            "listen": "::",
            "listen_port": 8080,
            "network": "tcp",
            "method": "2022-blake3-aes-128-gcm",
            "password": "[INPUT_SS_PASSWORD]",
            "multiplex": {
                "enabled": true,
                "padding": true
            }
        }
    ],
    "outbounds": [
        {
            "type": "direct"
        },
        {
            "type": "dns",
            "tag": "dns-out"
        }
    ],
    "route": {
        "rules": [
            {
                "protocol": "dns",
                "outbound": "dns-out"
            }
        ]
    }
}